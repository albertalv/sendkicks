<%- include ("../layouts/cabecera.ejs")%>

<form action="/tallas" method="post">
    <input id="my-input" value="<%=tenis.id%>" class="form-control" type="hidden" name="id">
         <select class="my-3" name="talla">
           <option value="">Selecciona la talla</option>
           <option value="tallaCinco">Talla 5</option>
           <option value="tallaSeis">Talla 6</option> 
           <option value="tallaSiete">Talla 7</option>
           <option value="tallaOcho">Talla 8</option>
           <option value="tallaNueve">Talla 9</option>
           <option value="tallaDiez">Talla 10</option>
       </select>
       <select class="my-3" name="cantidad">
         <option value="">Selecciona la cantidad</option>
       
         <option value="1">1</option>
         <option value="2">2</option> 
         <option value="3">3</option>
         <option value="4">4</option>
         <option value="5">5</option>
         <option value="6">6</option>
         <option value="7">7</option> 
         <option value="8">8</option>
         <option value="9">9</option>
         <option value="10">10</option>
     </select>
     <button class="btn btn-primary" type="submit">Guardar</button>
 </form>
